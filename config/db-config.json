{
  "development": {
    "url": "postgres://postgres:password@localhost:5432/postgres",
    "dialect": "postgres",
    "operatorsAliases": false,
    "dialectOptions": {
      "ssl": false,
      "multipleStatements": true
    }
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres",
    "operatorsAliases": false,
    "dialectOptions": {
      "ssl": true,
      "multipleStatements": true
    }
  }
}
